{
  "name": "admin-ui",
  "version": "0.0.1",
  "description": "Antd based component library for managing backend",
  "main": "es/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "dev": "father dev",
    "build": "father build",
    "build:deps": "father prebundle",
    "prepublishOnly": "father doctor && npm run build",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx  --fix --format=pretty ./packages && npm run lint:prettier && npm run check-deps",
    "lint:prettier": "npm run prettier &&  git diff && prettier --version && prettier --check \"packages/**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|md|mdx)\" --loglevel warn",
    "re-install": "pnpm run clean & pnpm install",
    "clean": "rm -rf ./node_modules ./dist ./es ./lib",
    "build:npm": "npm run clean && npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "ui",
    "antd"
  ],
  "authors": [
    "Yooco"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "es",
    "lib",
    "dev-config"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pengdongliang/react-iui"
  },
  "lint-staged": {
    "packages/**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@7.17.1",
  "peerDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "ahooks": "^3.7.4",
    "axios": "^1.3.3",
    "clsx": "^1.2.1",
    "lodash": "^4.17.21",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-activation": "^0.12.4",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.5",
    "resize-observer-polyfill": "^1.5.1",
    "use-http": "^1.0.27"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.6.0",
    "@babel/runtime": "^7.20.7",
    "antd": "^5.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@emotion/core": "^11.0.0",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/react-webpack5": "^7.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/addon-docs": "7.5.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.0",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "react-docgen-typescript-plugin": "^1.0.5",
    "babel-loader": "^8.3.0",
    "babel-plugin-import": "^1.13.6",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^13.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "father": "^4.3.5",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^6.0.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
